{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "removeComments": false,
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "./cjs","./esm","./umd", "./removeFiles.js"
  ]
}
